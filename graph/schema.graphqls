# https://gqlgen.com/getting-started/

type JobListing {
  _id: ID!,
  title: String!,
  description: String!,
  company: String!,
  url: String!
}

type Query {
  jobs: [JobListing!]!
  job(id: ID!): JobListing!
}

type Mutation {
  createJobListing(input: createJobListingInput)
}